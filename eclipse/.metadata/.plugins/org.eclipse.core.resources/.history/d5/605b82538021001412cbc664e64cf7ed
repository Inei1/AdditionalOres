package inei.additionalores.blocks;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;

public class Gemiumblock extends Block {

	public Gemiumblock(Material material) {
		super(material);
		setHardness(7.0F);
		setBlockName("WeakiumBlock");
		setStepSound(soundTypePiston);
        setHarvestLevel("pickaxe", 0);
        setBlockTextureName("aores:WeakiumBlock");
	}

}
