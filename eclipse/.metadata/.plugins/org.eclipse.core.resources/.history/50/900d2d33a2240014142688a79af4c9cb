package inei.additionalores.fluid;

import inei.additionalores.AdditionalOres;
import cpw.mods.fml.relauncher.Side;
import cpw.mods.fml.relauncher.SideOnly;
import net.minecraft.block.material.Material;
import net.minecraft.util.IIcon;
import net.minecraftforge.fluids.BlockFluidClassic;
import net.minecraftforge.fluids.Fluid;

public class BlockLiquidOre extends BlockFluidClassic{
	
	@SideOnly(Side.CLIENT)
    protected IIcon stillIcon;
    @SideOnly(Side.CLIENT)
    protected IIcon flowingIcon;
	
    public BlockLiquidOre(Fluid fluid, Material material) {
		super(fluid, material);
		setCreativeTab(AdditionalOres.tabOres);
	}
}
